<table>
    <% var i; for (i=0; i<quizes.length; i++){ %>
    <tr>
        <td>
            <a href="quizes/<%= quizes[i].id %>"><%=quizes[i].pregunta%></a>
        </td>
        <td>
            <a href="quizes/<%= quizes[i].id %>/edit"><button>editar</button></a>
        </td>
        
    </tr>
    <% } %>
    <tr>
        <td>
            <a href="quizes/new"><button type="button">Crear nueva pregunta</button></a>
        </td>
    </tr>
</table>
 <form method="get" action="/quizes">
    <p>
     Buscar Pregunta: <input type="text" name="search" value="Buscar..."/>
     <input type="submit" value="Enviar"/>
    </p>
 </form>
